{
  "connection": "default",
  "collectionName": "new_posts",
  "info": {
    "name": "new post"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "content": {
      "type": "enumeration",
      "enum": [
        "List",
        "EventListing",
        "Feature",
        "Interview",
        "Playlist",
        "News",
        "Other"
      ],
      "default": "List",
      "required": true
    },
    "Title": {
      "type": "string",
      "required": true
    },
    "blurb": {
      "type": "richtext",
      "required": true
    },
    "body": {
      "type": "richtext",
      "required": true
    }
  }
}
